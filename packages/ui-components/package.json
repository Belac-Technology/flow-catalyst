{
  "name": "@flowcatalyst/ui-components",
  "version": "0.1.0",
  "description": "FlowCatalyst UI component library built with Vue 3, TypeScript, and Tailwind CSS",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build && vue-tsc --declaration --emitDeclarationOnly",
    "type-check": "vue-tsc --noEmit"
  },
  "peerDependencies": {
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.0",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.0",
    "vue": "^3.4.0",
    "vue-tsc": "^1.8.27"
  },
  "keywords": [
    "flowcatalyst",
    "vue",
    "components",
    "tailwind",
    "ui",
    "event-driven"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/flowcatalyst.git",
    "directory": "packages/ui-components"
  },
  "license": "Apache-2.0"
}
